{
  "name": "@bossnyumba/api-client",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./hooks": {
      "require": "./dist/hooks.js",
      "import": "./dist/hooks.mjs",
      "types": "./dist/hooks.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/hooks.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/hooks.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@bossnyumba/domain-models": "workspace:*",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.48",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
